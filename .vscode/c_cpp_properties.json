{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr/**",
                "${env:HOME}/.arduino15/packages/arduino/tools/avr-gcc/**",
                "${workspaceFolder}/**",
                "/usr/avr/sys-root/include/"
            ],
            "forcedInclude": [
                "${env:HOME}/.arduino15/packages/arduino/hardware/avr1.8.3/cores/arduino/Arduino.h"
            ],
            "defines": [ "UBRRH" ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}